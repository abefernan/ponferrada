{
  "name": "valdueza-storybook",
  "version": "1.1.1-alpha.8",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "firebase deploy --project valdueza-storybook --only hosting:valdueza-storybook",
