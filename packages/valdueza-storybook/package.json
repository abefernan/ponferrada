    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "babel-loader": "^8.0.6",
    "medulas-react-components": "^1.1.1-alpha.7"
  },
  "devDependencies": {
    "@iov/browser-extension-react-scripts": "^3.3.0",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4"
  }
}
